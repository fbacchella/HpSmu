<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
  <name>HpSmuCtrlCpu</name>
  <graphName>hpsmuctrlcpu-${attr.controller}</graphName>
  <graphTitle>CPU usage on controller ${attr.controller} on ${host}</graphTitle>
  <verticalLabel>%</verticalLabel>
  <add>
    <name>cpu-load</name>
    <graphType>line</graphType>
    <legend>Percentage of time the CPU is busy</legend>
  </add>
  <hosttree>
    <pathelement>HOST</pathelement>
    <pathstring>Controllers activity</pathstring>
    <pathstring>${attr.controller}</pathstring>
    <pathstring>CPU usage</pathstring>
  </hosttree>
  <viewtree>
    <pathstring>HP Storage</pathstring>
    <pathstring>Controllers activity</pathstring>
    <pathstring>CPU usage</pathstring>
    <pathelement>HOST</pathelement>
    <pathstring>${attr.controller}</pathstring>
  </viewtree>
</graphdesc>
